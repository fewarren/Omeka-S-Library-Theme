# Rate Limiting Configuration
# Copy this file to .env or add these variables to your environment

# Redis Configuration (Primary choice for rate limiting)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_TIMEOUT=2.0

# Memcached Configuration (Fallback option)
MEMCACHED_HOST=localhost
MEMCACHED_PORT=11211

# Rate Limiting Settings (optional - can be set in code)
RATE_LIMIT_MAX_REQUESTS=60
RATE_LIMIT_TIME_WINDOW=3600

